@{
    ViewBag.Title = "Home Page";
    var userAccount = ViewBag.UserAccount;

}

@{
    if (userAccount == 1)
    {
        @Html.Action("UserManagementPartialView", "User")
    }
    else if (userAccount == 2)
    {
        @Html.Action("BookLibraryPartialView", "Book")
    }
    else
    {
        @Html.Action("StorePartialView", "Store")
    }

}

